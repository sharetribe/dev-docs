{
  "name": "dev-docs",
  "version": "1.0.0",
  "repository": "https://github.com/sharetribe/dev-docs.git",
  "author": "Aleksi O'Connor <aleksi@sharetribe.com>",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "next": "^15.5.2",
    "next-sitemap": "^4.2.3",
    "nextra": "^4.4.0",
    "nextra-theme-docs": "^4.4.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "dev": "next",
    "build": "next build && mv out out-root && mkdir out && mv out-root out/docs && mv out/docs/404.html out/",
    "start": "npx serve@latest out",
    "postbuild": "next-sitemap && pagefind --site out/docs --output-path out/docs/_pagefind",
    "format": "prettier . --write",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/node": "24.0.3",
    "@types/react": "19.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "pagefind": "^1.3.0",
    "prettier": "3.6.2"
  }
}
